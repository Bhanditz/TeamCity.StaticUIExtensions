<?xml version="1.0" ?>
<teamcity-static-ui-extensions>

  <rule place-id="ALL_PAGES_HEADER" html-file="header.html">
     <url startsWith="overview.html" />
  </rule>
                                                                            
  <rule place-id="ALL_PAGES_FOOTER" html-file="footer.html" css-file="footer.css" js-file="footer.js" />
                                  	

  <rule place-id="BEFORE_CONTENT" js-file="global.js" />
  <rule place-id="BEFORE_CONTENT" css-file="global.css" />

</teamcity-static-ui-extensions>